{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "buildCommand": "pnpm install && pnpm run build",
  "installCommand": "pnpm install",
  "env": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "info"
  }
}
